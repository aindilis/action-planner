		(and (PRICE_27 ?x1) (be_27 ?e1 ?x1 ?x2) (LESS_27 ?x2))

	;; INDIVIDUAL -S MAY ADD A MARK-UP TO A TICKET.

		(and (INDIVIDUAL_27 ?x1) (kbs2_kif_quote_2DS_27 ?x1) (MAY_27 ?x1) (ADD_27 ?x1) (A_27 ?x1) (MARK-UP_27 ?x1) (to_27 ?x1 ?x2) (A_27 ?x2) (TICKET_27 ?x2))

