		(and (MARK-UP_27 ?x1) (be_27 ?e1 ?x1 ?x2) (SLIGHT_27 ?x2))

	;; A PERSON MAY PAY.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (PAY_27 ?x1))

	;; A PERSON MAY PURCHASE TICKETS.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (PURCHASE_27 ?x1) (TICKETS_27 ?x1))

