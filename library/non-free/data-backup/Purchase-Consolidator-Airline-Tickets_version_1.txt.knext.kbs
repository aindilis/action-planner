	;; TRAVEL AGENTS MAY HAVE A LIST OF CONSOLIDATORS.

		(and (TRAVEL_27 ?x1) (AGENTS_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (LIST_27 ?x1) (of_27 ?x1 ?x2) (CONSOLIDATORS_27 ?x2))

