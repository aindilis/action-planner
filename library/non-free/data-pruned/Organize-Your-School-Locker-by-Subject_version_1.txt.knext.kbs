		(and (SUBJECT_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (TEXTBOOK_27 ?x1) (COVER_27 ?x1))

;; (REPEAT FOR EVERY SUBJECT)

	;; REPEAT CAN BE FOR SOME_NUMBER_OF SUBJECT.

		(and (repeat_27 ?x1) (be_27 ?e1 ?x1) (for_27 ?x1 ?x2) (SOME_5FNUMBER_5FOF_27 ?x2) (SUBJECT_27 ?x2))

