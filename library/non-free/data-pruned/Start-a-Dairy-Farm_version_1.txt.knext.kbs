	;; A QUOTA CAN BE CONSTITUTED_OF_OR_FILLED_WITH SOME_NUMBER_OF COWS.

		(and (QUOTA_27 ?x1) (be_27 ?e1 ?x1 ?x2) (CONSTITUTED_5FOF_5FOR_5FFILLED_5FWITH_27 ?x2) (SOME_5FNUMBER_5FOF_27 ?x2) (COWS_27 ?x2))

;; (-LRB- QUOTA MEANS MINIMUM ALLOWABLE AMOUNT OF ANIMALS TO START AND TO HAVE IN DAIRY PRODUCTION)

