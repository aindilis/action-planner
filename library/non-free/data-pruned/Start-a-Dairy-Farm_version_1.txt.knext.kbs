;; (IF YOU ARE BUYING A PIECE OF LAND THAT YOU NEED TO BUILD BUILDINGS ON COMMA THEN MAKE SURE YOU BUILD SILOS COMMA BARNS COMMA MANURE HOLDING LAGOONS OR SLOUGHS COMMA CORRALS COMMA STANCHIONS FOR COWS FOR MILKING
 -LRB- TYPICALLY IN THE MILK PARLOR -RRB- AND AREA WHERE IT IS EASY TO AI COWS)

	;; A PERSON MAY BUY A PIECE OF LAND A PROPOSITION.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (BUY_27 ?x1) (A_27 ?x1) (PIECE_27 ?x1) (of_27 ?x1 ?x2) (LAND_27 ?x2) (A_27 ?x2) (PROPOSITION_27 ?x2))

	;; A PIECE CAN BE CONSTITUTED_OF_OR_FILLED_WITH LAND.

		(and (PIECE_27 ?x1) (be_27 ?e1 ?x1 ?x2) (CONSTITUTED_5FOF_5FOR_5FFILLED_5FWITH_27 ?x2) (LAND_27 ?x2))

	;; A PERSON MAY BUILD SILOS.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (BUILD_27 ?x1) (SILOS_27 ?x1))

	;; HOLDING LAGOONS CAN BE MANURE.

		(and (HOLDING_27 ?x1) (LAGOONS_27 ?x1) (be_27 ?e1 ?x1 ?x2) (MANURE_27 ?x2))

	;; STANCHIONS CAN BE FOR COWS.

		(and (stanchion_27 ?x1) (be_27 ?e1 ?x1) (for_27 ?x1 ?x2) (cow_27 ?x2))

;; (YOU WILL ALSO NEED A CALVING FACILITY COMMA AND A FACILITY WHERE YOU CAN KEEP THE CALVES YOU HAVE TO PULL OFF THE COWS SO THEY CAN BE BOTTLE FED AND THE COWS GO INTO MILK PRODUCTION)

	;; A PERSON MAY NEED A FACILITY.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (NEED_27 ?x1) (A_27 ?x1) (FACILITY_27 ?x1))

	;; A FACILITY CAN BE CALVING.

		(and (FACILITY_27 ?x1) (be_27 ?e1 ?x1 ?x2) (CALVING_27 ?x2))

	;; A FACILITY MAY GO INTO MILK PRODUCTION.

		(and (FACILITY_27 ?x1) (MAY_27 ?x1) (GO_27 ?x1) (INTO_27 ?x1) (MILK_27 ?x1) (production_27 ?x1))

	;; A PERSON MAY KEEP CALVES.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (KEEP_27 ?x1) (CALVES_27 ?x1))

	;; INDIVIDUAL -S CAN BE BOTTLE FED.

		(and (INDIVIDUAL_27 ?x1) (kbs2_kif_quote_2DS_27 ?x1) (be_27 ?e1 ?x1 ?x2) (BOTTLE_27 ?x2) (FED_27 ?x2))

;; (MAKE SURE THE SIZE OF THE BUILDINGS MATCH THE SIZE OF HERD YOU ARE WISHING TO NOT START WITH COMMA BUT HAVE WHEN YOU HAVE REACHED YOUR MAXIMUM DAIRY HERD SIZE)

	;; A PERSON MAY REACH A DAIRY HERD SIZE.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (REACH_27 ?x1) (A_27 ?x1) (DAIRY_27 ?x1) (HERD_27 ?x1) (SIZE_27 ?x1))

	;; A PERSON MAY HAVE A DAIRY HERD SIZE.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (DAIRY_27 ?x1) (HERD_27 ?x1) (SIZE_27 ?x1))

;; (FIND A COMPANY YOU CAN SELL YOUR MILK TO)

	;; A PERSON MAY HAVE MILK.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (MILK_27 ?x1))

;; (YOU MAY ALSO HAVE TO GO THROUGH SOME PAPERWORK SENT OUT BY THE GOVERNMENT TOO COMMA AMONG OTHER THINGS)

	;; SOME_NUMBER_OF PAPERWORK MAY BE SEND_OUT -ED BY A GOVERNMENT.

		(and (SOME_5FNUMBER_5FOF_27 ?x1) (PAPERWORK_27 ?x1) (MAY_27 ?x1) (be_27 ?e1 ?x1 ?x2) (SEND_5FOUT_27 ?x2) (kbs2_kif_quote_2DED_27 ?x2) (BY_27 ?x2) (A_27 ?x2) (GOVERNMENT_27 ?x2))

