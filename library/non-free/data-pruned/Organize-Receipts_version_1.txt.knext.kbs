;; (AWAYON A LEGAL PAD COMMA MAKE A LIST OF CATEGORIES AND SUBCATEGORIES FOR YOUR RECEIPTS)

	;; A PAD CAN BE LEGAL.

		(and (PAD_27 ?x1) (be_27 ?e1 ?x1 ?x2) (LEGAL_27 ?x2))

	;; A LIST CAN BE CONSTITUTED_OF_OR_FILLED_WITH CATEGORIES.

		(and (LIST_27 ?x1) (be_27 ?e1 ?x1 ?x2) (CONSTITUTED_5FOF_5FOR_5FFILLED_5FWITH_27 ?x2) (CATEGORIES_27 ?x2))

	;; A PERSON MAY HAVE RECEIPTS.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (RECEIPTS_27 ?x1))

;; (SUGGESTIONS FOR CATEGORIES CAN BE EASILY ADAPTED FROM PROGRAMS SUCH AS QUICKEN OR MSMONEY PURCHASE INEXPENSIVE FILE FOLDERS FOR EACH MAIN
 CATEGORY)

	;; SUGGESTIONS MAY BE ADAPT -ED FROM PROGRAMS.

		(and (suggestion_27 ?x1) (be_27 ?e1 ?x1 ?x2) (ADAPT_27 ?x2) (kbs2_kif_quote_2DED_27 ?x2) (FROM_27 ?x2) (PROGRAMS_27 ?x2))

	;; SUGGESTIONS CAN BE FOR CATEGORIES.

		(and (suggestion_27 ?x1) (be_27 ?e1 ?x1) (for_27 ?x1 ?x2) (category_27 ?x2))

	;; A QUICKEN_OR_MSMONEY_PURCHASE[PERSON??] MAY HAVE INEXPENSIVE FILE FOLDERS.

		(and (QUICKEN_5FOR_5FMSMONEY_5FPURCHASE_27 ?x1) (kbs2_kif_quote_5B_27 ?x1) (PERSON_27 ?x1))

		(and (MAY_27 ?x1) (HAVE_27 ?x1) (INEXPENSIVE_27 ?x1) (FILE_27 ?x1) (folder_27 ?x1))

;; (LABEL THE TAB OF THE FOLDER WITH THE NAME OF THE MAIN CATEGORY)

	;; A FOLDER MAY HAVE A TAB.

		(and (FOLDER_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (TAB_27 ?x1))

	;; A FOLDER CAN BE WITH A NAME OF A CATEGORY.

		(and (folder_27 ?x1) (be_27 ?e1 ?x1 ?x2) (WITH_27 ?x2) (A_27 ?x2) (NAME_27 ?x2) (of_27 ?x2 ?x3) (A_27 ?x3) (CATEGORY_27 ?x3))

	;; A CATEGORY MAY HAVE A NAME.

		(and (CATEGORY_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (NAME_27 ?x1))

;; (ON THE FRONT FACE OF THE FOLDER COMMA WRITE THE SUBCATEGORIES THAT WILL BE CONTAINED IN THAT FOLDER)

	;; A FOLDER MAY HAVE A FACE.

		(and (FOLDER_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (FACE_27 ?x1))

;; (SORT FOLDERS ALPHABETICALLY BY CATEGORY)

	;; FOLDERS MAY ALPHABETICALLY BY CATEGORY.

		(and (folder_27 ?x1) (MAY_27 ?x1) (ALPHABETICALLY_27 ?x1) (BY_27 ?x1) (CATEGORY_27 ?x1))

	;; FOLDERS CAN BE SORT.

		(and (folder_27 ?x1) (be_27 ?e1 ?x1 ?x2) (SORT_27 ?x2))

;; (START AT THE TOP OF YOUR UNSORTED PILE AND HANDLE EACH RECEIPT ONLY ONCE)

	;; START MAY PILE.

		(and (START_27 ?x1) (MAY_27 ?x1) (PILE_27 ?x1))

	;; START CAN BE AT A TOP OF AN UNSORTED.

		(and (START_27 ?x1) (be_27 ?e1 ?x1) (at_27 ?x1 ?x2) (A_27 ?x2) (TOP_27 ?x2) (of_27 ?x2 ?x3) (UNSORTED_27 ?x3))

	;; AN UNSORTED MAY HAVE A TOP.

		(and (UNSORTED_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (TOP_27 ?x1))

	;; A PERSON MAY HAVE AN UNSORTED.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (AN_27 ?x1) (UNSORTED_27 ?x1))

;; (AS YOU SORT THROUGH THE RECEIPTS COMMA LOOK AT THE DATE FIRST AND REMOVE ANY THAT ARE NOT FOR THE CURRENT YEAR)

	;; A PERSON MAY SORT THROUGH RECEIPTS.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (SORT_27 ?x1) (THROUGH_27 ?x1) (RECEIPTS_27 ?x1))

;; (YOU SHOULD USE A DIFFERENT SET OF FILES FOR DIFFERENT TAX YEARS)

	;; A PERSON MAY USE A SET OF FILES.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (USE_27 ?x1) (A_27 ?x1) (SET_27 ?x1) (of_27 ?x1 ?x2) (file_27 ?x2))

	;; A SET CAN BE CONSTITUTED_OF_OR_FILLED_WITH FILES.

		(and (SET_27 ?x1) (be_27 ?e1 ?x1 ?x2) (CONSTITUTED_5FOF_5FOR_5FFILLED_5FWITH_27 ?x2) (FILES_27 ?x2))

	;; FILES CAN BE FOR TAX YEARS.

		(and (file_27 ?x1) (be_27 ?e1 ?x1) (for_27 ?x1 ?x2) (TAX_27 ?x2) (year_27 ?x2))

;; (DETERMINE THE MOST APPROPRIATE CATEGORY OR SUBCATEGORY THAT WOULD APPLY TO THE RECEIPT)

	;; A CATEGORY CAN BE APPROPRIATE.

		(and (CATEGORY_27 ?x1) (be_27 ?e1 ?x1 ?x2) (APPROPRIATE_27 ?x2))

;; (PLACE RECEIPT IN THE CORRESPONDING FOLDER)

	;; A FOLDER CAN BE CORRESPONDING.

		(and (folder_27 ?x1) (be_27 ?e1 ?x1 ?x2) (corresponding_27 ?x2))

;; (IF YOU DISCOVER NEW CATEGORIES COMMA ADD NEW FOLDERS)

	;; A PERSON MAY DISCOVER CATEGORIES.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (DISCOVER_27 ?x1) (CATEGORIES_27 ?x1))

;; (IF YOU DISCOVER NEW SUBCATEGORIES COMMA WRITE THEM ON THE FRONT OF THE CATEGORY FOLDER)

	;; A PERSON MAY DISCOVER SUBCATEGORIES.

		(and (PERSON_27 ?x1) (MAY_27 ?x1) (DISCOVER_27 ?x1) (SUBCATEGORIES_27 ?x1))

	;; A CATEGORY FOLDER MAY HAVE A FRONT.

		(and (CATEGORY_27 ?x1) (FOLDER_27 ?x1) (MAY_27 ?x1) (HAVE_27 ?x1) (A_27 ?x1) (FRONT_27 ?x1))
